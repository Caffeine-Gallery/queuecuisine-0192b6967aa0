type MenuItem = 
 record {
   description: text;
   id: nat;
   name: text;
   price: nat;
 };
type Customer = 
 record {
   name: text;
   phoneNumber: text;
 };
service : {
  addMenuItem: (text, nat, text) -> (nat);
  addToWaitlist: (text, text) -> (nat);
  getMenu: () -> (vec MenuItem) query;
  getWaitlist: () -> (vec Customer) query;
  getWaitlistPosition: (text) -> (opt nat) query;
  removeFromWaitlist: (text) -> (bool);
}
